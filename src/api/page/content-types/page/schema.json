{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Page"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "Content": {
      "type": "dynamiczone",
      "components": [
        "shared.title",
        "shared.paragraph",
        "shared.media"
      ]
    },
    "page_group": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::page-group.page-group",
      "inversedBy": "relatedpages"
    }
  }
}
