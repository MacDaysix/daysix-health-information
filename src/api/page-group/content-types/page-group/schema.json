{
  "kind": "collectionType",
  "collectionName": "page_groups",
  "info": {
    "singularName": "page-group",
    "pluralName": "page-groups",
    "displayName": "PageGroup"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "relatedpages": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::page.page",
      "mappedBy": "page_group"
    },
    "slug": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "BackgroundColor": {
      "type": "customField",
      "customField": "plugin::color-picker.color",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$"
    }
  }
}
